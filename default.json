{
    "$schema": "https://docs.renovatebot.com/renovate-schema.json",
    "extends": [
        "config:base",
        "group:allNonMajor",
        ":semanticCommits",
        ":combinePatchMinorReleases",
        ":rebaseStalePrs",
        ":prConcurrentLimit10",
        ":prHourlyLimit1",
        ":disableDependencyDashboard"
    ],
    "packageRules": [
        {
            "matchDepTypes": [
                "devDependencies"
            ],
            "prPriority": -1
        }
    ]
}